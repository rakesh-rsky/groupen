          <!-- <span class="icon-bars"></span> -->
        </button>
      </div>

      <div id="navbar" class="navbar-collapse collapse">
        <div class="userlist-name">
          <span id="toggle-close-btn1">&times;</span>
          <ul class="nav navbar-nav user-list nano-content">

          </ul>
        </div>
      </div>

      <div id="user-panel" class="navbar-collapse collapse">
        <div class="user-panel-list">
          <span id="toggle-close-btn2">&times;</span>
          <ul class="nav navbar-nav user-setting-list nano-content">
            <li class="setting-list" id="search">
              <span
                class="glyphicon glyphicon-search user-search-logo"
              ></span>search
            </li>
            <li class="setting-list" id="chat-request">
              <span
                class="glyphicon glyphicon-envelope user-chat-list"
              ></span>Friend Request
            </li>
            <li class="setting-list" id="friend-list">
              <span
                class="glyphicon glyphicon-user user-friend-list"
              ></span>Friend List
            </li>
            <li class="setting-list" id="setting">
              <span class="glyphicon glyphicon-cog user-set-list"></span>Setting
            </li>
            <li class="setting-list" id="logout">
              <span
                class="glyphicon glyphicon-log-out user-set-list"
              ></span>Logout
            </li>

            <li class="setting-list" id="about">
              <span
                class="glyphicon glyphicon-info-sign user-set-list"
              ></span>About
            </li>
          </ul>
        </div>
      </div>

    </div>

    <!-- <div>
      <ul id="messages"></ul>
    </div> -->
    <div id="chat_main">
    </div>

  </div>

  <div class="col-md-2 aboutus-main">
    <div class="about-us">
      <a href="/about-us" target="">About-us</a>
    </div>
  </div>

</div>

<form class="type_message hiding" action="">
  <input
    id="m2"
    autocomplete="off"
    placeholder=" Type a message "
    disabled="disabled"
  />
  <input id="m" autocomplete="off" placeholder=" Type a message " />
  <a id="send-location"><span
      class="glyphicon glyphicon-map-marker location"
    ></span></a>
  {{! id="send-file" renamed to "send-file-renamed" to prevent click on paperclip }}
  <a id="send-file-renamed"><span
      class="glyphicon glyphicon-paperclip paperclip"
    ></span></a>
  <button id="btn"></button>
</form>

<div>
  <div
    id="signup-modal"
    class="modal fade reg"
    role="dialog"
    data-backdrop="static"
    data-keyboard="false"
  >
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-body">
          <a
            href="/about-us"
            target=""
            class="btn btn-success aboutlink"
          >About</a>
          <button class="btn btn-success signup-to-login">Login</button>
          <form
            id="signup-form"
            action="/signup"
            method="post"
            enctype="multipart/form-data"
          >
            <h1 class="modal-heading">Sign Up</h1>
            <div class="form-group">
              <input
                type="text"
                class="form-control name"
                name="name"
                placeholder="Enter name*"
              />
            </div>
            <div class="form-group">
              <input
                id="username"
                type="text"
                class="form-control username"
                name="username"
                placeholder="Enter username*"
              />
              <span class="phone-info username-info"></span>

            </div>
            <div class="form-group">
              <input
                type="password"
                class="form-control password"
                name="password"
                placeholder="Enter Password*"
              />
            </div>
            <div class="form-group">
              <input
                type="text"
                class="form-control email"
                name="email"
                placeholder="Enter Email*"
              />
              <span class="phone-info">we send OTP to this email</span>
            </div>
            <div class="form-group">
              <button class="btn btn-success signup-btn">sign up</button>
            </div>
          </form>
        </div>

      </div>
    </div>
  </div>
</div>

<div>
  <div
    id="login-modal"
    class="modal fade reg"
    role="dialog"
    data-backdrop="static"
    data-keyboard="false"
  >
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-body">
          <a
            href="/about-us"
            target=""
            class="btn btn-success aboutlink"
          >About</a>
          <button class="btn btn-success login-to-signup">Sign up</button>
          <form
            id="login-form"
            action="/login"
            method="post"
            enctype="multipart/form-data"
          >
            <h1 class="modal-heading">Login</h1>
            <div class="form-group">
              <input
                type="text"
                class="form-control username tmp-user"
                data-id="{{loggedUsername}}"
                name="username"
                value="{{loggedUsername}}"
                placeholder="Enter your username"
              />
            </div>
            <div class="form-group">
              <input
                type="password"
                class="form-control password tmp-text"
                data-id="{{loggedPassword}}"
                name="password"
                value="{{loggedPassword}}"
                placeholder="Enter your password"
              />
            </div>
            <div class="form-group">
              <span
                style="display: none;"
                class="alert alert-danger login-info"
              ></span>
            </div>
            <div class="form-group">
              <button class="btn btn-success login-btn">Login</button>
            </div>
          </form>
        </div>

      </div>
    </div>
  </div>
</div>

<div>
  <div
    id="search-modal"
    class="modal fade search-user-main"
    role="dialog"
    data-backdrop="static"
    data-keyboard="false"
