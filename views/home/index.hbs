<div>
  <div
    id="setting-modal"
    class="modal fade setting-main"
    role="dialog"
    data-backdrop="static"
    data-keyboard="false"
  >
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-body">
          <div id="setting-list">
            <ul>
              <li
                id="change-name"
                class="alert alert-success setting_list"
              >Change Name</li>
              <div id="name-change">
                <div class="form-group">
                  <input
                    type="text"
                    id="new-name"
                    class="form-control"
                    name="newname"
                    placeholder="Enter new Name"
                  />
                </div>
                <div class="form-group">
                  <span
                    style="display: none;"
                    class="alert newname-info"
                  ></span>
                </div>
                <div class="form-group">
                  <button
                    type="button"
                    id="name-change-btn"
                    class="btn btn-success name-change-btn"
                  >Save</button>
                </div>
              </div>
              <hr />

              <li
                id="change-email"
                class="alert alert-success setting_list"
              >Change Email</li>
              <div id="email-change">
                <div class="form-group">
                  <input
                    type="text"
                    id="new-email"
                    class="form-control"
                    name="newemail"
                    placeholder="Enter New Email"
                  />
                </div>
                <div class="form-group">
                  <span
                    style="display: none;"
                    class="alert newemail-info"
                  ></span>
                </div>
                <div class="form-group">
                  <button
                    type="button"
                    id="email-change-btn"
                    class="btn btn-success email-change-btn"
                  >Save</button>
                </div>
              </div>
              <hr />

              <li
                id="change-password"
                class="alert alert-success setting_list"
              >Change Password</li>
              <div id="password-change">
                <div class="form-group">
                  <input
                    type="password"
                    id="old-password"
                    class="form-control"
                    name="oldpassword"
                    placeholder="Enter old Password"
                  />
                </div>
                <div class="form-group">
                  <input
                    type="password"
                    id="new-password"
                    class="form-control"
                    name="newpassword"
                    placeholder="Enter New Password"
                  />
                </div>
                <div class="form-group">
                  <span
                    style="display: none;"
                    class="alert newpassword-info"
                  ></span>
                </div>
                <div class="form-group">
                  <button
                    type="button"
                    id="password-change-btn"
                    class="btn btn-success password-change-btn"
                  >change</button>
                </div>
              </div>
              <hr />
            </ul>
          </div>

          <div class="form-group">
            <button
              class="btn btn-info setting-close-btn"
              class="search-list"
            >close</button>
          </div>

        </div>
      </div>
    </div>
  </div>
</div>

